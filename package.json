{
  "name": "react-from-svg",
  "version": "1.1.0",
  "license": "MIT",
  "repository": "https://github.com/MoOx/react-from-svg",
  "keywords": [
    "react",
    "react-native",
    "reason-react",
    "bs-react",
    "svg",
    "react-native-svg",
    "react-native-web",
    "bucklescript",
    "react-native",
    "svg-to-react",
    "svgr"
  ],
  "bin": {
    "react-from-svg": "./bin.js"
  },
  "files": [
    "src",
    "!test",
    "bin.js",
    "bsconfig.json"
  ],
  "scripts": {
    "postinstall": "bsb -clean-world -make-world",
    "re:clean": "bsb -clean-world",
    "re:build": "bsb -make-world",
    "svg:format": "find test/components -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "svg": "trash test/components && ./bin.js test test/components --with-reason && yarn svg:format",
    "test": "yarn re:build && yarn svg",
    "release": "npmpub"
  },
  "dependencies": {
    "bs-platform": "^5.0.1",
    "glob": "^7.1.3",
    "mkdirp": "^0.5.1",
    "reason-future": "^2.4.0",
    "reason-react": "^0.7.0"
  },
  "devDependencies": {
    "npmpub": "^5.0.0",
    "trash-cli": "^1.4.0"
  }
}
